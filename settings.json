    // Update Date: 21/05/2021
    // *********************************************************
    // === VSCode Recommended Settings For Laravel & Vue.js ===
    // *********************************************************

    "editor.mouseWheelZoom": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.fontFamily": "Fira Code, Operator Mono, JetBrains Mono, Consolas, monospace",
    "prettier.trailingComma": "none",
    "editor.formatOnSave": true,
    "prettier.printWidth": 500,
    "prettier.tabWidth": 4,
    "prettier.singleQuote": true,
    "diffEditor.wordWrap": "off",
    "editor.wordWrapColumn": 500,
    "workbench.tree.indent": 15,
    "explorer.compactFolders": false,
    "editor.tabSize": 4,
    "php.validate.executablePath": "C:/xampp/php/php.exe",
    "php.suggest.basic": false,
    "editor.linkedEditing": true,
    "[blade]": {
        "editor.defaultFormatter": "shufo.vscode-blade-formatter"
    },
    "bladeFormatter.format.wrapLineLength": 500,
    "terminal.integrated.cursorWidth": 5,
    "editor.minimap.enabled": false,
    "explorer.confirmDelete": false,
    "editor.cursorWidth": 4,
    "editor.lineHeight": 30,
    "terminal.integrated.fontFamily": "Operator Mono",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontWeight": "520",
    "peacock.favoriteColors": [
        { "name": "June Bud", "value": "#badc58" },
        { "name": "Quince Jelly", "value": "#f0932b" },
        { "name": "Heliotrope", "value": "#e056fd" },
        { "name": "Pure Apple", "value": "#6ab04c" },
        { "name": "Hint Of Ice Pack", "value": "#c7ecee" },
        { "name": "Greenland Green", "value": "#22a6b3" },
        { "name": "Turbo", "value": "#f9ca24" },
        { "name": "Sun Flower", "value": "#f1c40f" },
        { "name": "Pumpkin", "value": "#d35400" },
        { "name": "Orange", "value": "#f39c12" },
        { "name": "Wisteria", "value": "#8e44ad" },
        { "name": "Midnight Blue", "value": "#2c3e50" },
        { "name": "Matt Purple", "value": "#8c7ae6" },
        { "name": "Blueberry Soda", "value": "#7f8fa6" },
        { "name": "Ufo Green", "value": "#2ed573" },
        { "name": "Lighter Purple", "value": "#a55eea" },
        { "name": "Dark Periwinkle", "value": "#575fcf" },
        { "name": "Minty Green", "value": "#0be881" }
    ],
    "editor.fontWeight": "500",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBorder": "#8e44ad",
        "icon.foreground": "#f7b731",
        "tab.activeBackground": "#305204",
        "tab.hoverBorder": "#ff0000",
        "progressBar.background": "#ff0000",
        "tree.indentGuidesStroke": "#009432",
        "editorIndentGuide.activeBackground": "#44bd32",
        "editorWhitespace.foreground": "#f39c12",
        "[Quiet Light]": {
            "editorCursor.foreground": "#c0392b",
            "editorIndentGuide.activeBackground": "#c23616",
            "editor.selectionBackground": "#4cd137",
            "editor.selectionHighlightBackground": "#4cd137",
            "editor.findMatchBackground": "#00cc44a8",
            "editor.findMatchHighlightBackground": "#ff7b00a1",
            "terminal.background": "#dfe6e9",
            "terminal.foreground": "#2c3e50",
            "terminalCursor.foreground": "#1e1ed1",
            "terminal.selectionBackground": "#f39c12",
            "scrollbarSlider.activeBackground": "#e74c3c",
            "scrollbarSlider.background": "#8c7ae6",
            "scrollbarSlider.hoverBackground": "#2ecc71",
            "editorBracketMatch.background": "#F79F1F",
            "editor.background": "#ecf0f1"
        },
        "[Solarized Dark]": {
            "editor.selectionBackground": "#aaa69d",
            "editor.selectionHighlightBackground": "#aaa69d",
            "editor.findMatchBackground": "#f368e0",
            "editor.findMatchHighlightBackground": "#e1b12c",
            "scrollbarSlider.activeBackground": "#e74c3c",
            "scrollbarSlider.background": "#8c7ae6",
            "scrollbarSlider.hoverBackground": "#2ecc71",
            "editorIndentGuide.activeBackground": "#6ab04c"
        }
    },
    "editor.tokenColorCustomizations": {
        "[Quiet Light]": {
            "comments": "#c0392b",
            "strings": "#009432",
            "textMateRules": [
                {
                    "scope": "punctuation",
                    "settings": {
                        "foreground": "#1B1464"
                    }
                },
                {
                    "scope": "keyword.operator",
                    "settings": {
                        "foreground": "#0652DD"
                    }
                },
                {
                    "scope": "entity.name.tag",
                    "settings": {
                        "foreground": "#e67e22"
                    }
                },
                {
                    "scope": "string.quoted.double.html",
                    "settings": {
                        "foreground": "#be2edd"
                    }
                },
                {
                    "scope": "keyword.operator.key.php",
                    "settings": {
                        "foreground": "#FF0000"
                    }
                },
                {
                    "scope": "punctuation.definition.tag.begin.html",
                    "settings": {
                        "foreground": "#5f27cd"
                    }
                },
                {
                    "scope": "punctuation.definition.tag.end.html",
                    "settings": {
                        "foreground": "#5f27cd"
                    }
                },
                {
                    "scope": "entity.other.attribute-name.html",
                    "settings": {
                        "foreground": "#0abde3",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": "support.function",
                    "settings": {
                        "foreground": "#44bd32"
                    }
                },
                {
                    "scope": "keyword.blade",
                    "settings": {
                        "foreground": "#2c2c54"
                    }
                },
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "foreground": "#130f40"
                    }
                },
                {
                    "scope": "meta.tag",
                    "settings": {
                        "foreground": "#192a56"
                    }
                },
                {
                    "scope": "keyword.control.return.php",
                    "settings": {
                        "foreground": "#c23616",
                        "fontStyle": "italic"
                    }
                }
            ]
        }
    },
    "editor.fontSize": 15,
    "workbench.colorTheme": "Atom One Dark"
}
